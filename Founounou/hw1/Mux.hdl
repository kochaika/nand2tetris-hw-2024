/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in= sel, out= notSel);
    And(a= a, b= notSel, out= first);
    And(a= sel, b= b, out= second);
    Or(a= first, b= second, out= out);
}