/**
 * 8-way demultiplexor:
 * [a, b, c, d, e, f, g, h] = [in, 0,  0,  0,  0,  0,  0,  0] if sel = 000
 *                            [0, in,  0,  0,  0,  0,  0,  0] if sel = 001
 *                            [0,  0, in,  0,  0,  0,  0,  0] if sel = 010
 *                            [0,  0,  0, in,  0,  0,  0,  0] if sel = 011
 *                            [0,  0,  0,  0, in,  0,  0,  0] if sel = 100
 *                            [0,  0,  0,  0,  0, in,  0,  0] if sel = 101
 *                            [0,  0,  0,  0,  0,  0, in,  0] if sel = 110
 *                            [0,  0,  0,  0,  0,  0,  0, in] if sel = 111
 */
CHIP DMux8Way {
    IN in, sel[3];
    OUT a, b, c, d, e, f, g, h;

    PARTS:
    DMux(in= in, sel= sel[2], a= result1, b= result2);

    DMux(in= result1, sel= sel[1], a= result3, b= result4);
    DMux(in= result2, sel= sel[1], a= result5, b= result6);

    DMux(in= result3, sel= sel[0], a= a, b= b);
    DMux(in= result4, sel= sel[0], a= c, b= d);
    DMux(in= result5, sel= sel[0], a= e, b= f);
    DMux(in= result6, sel= sel[0], a= g, b= h);
}